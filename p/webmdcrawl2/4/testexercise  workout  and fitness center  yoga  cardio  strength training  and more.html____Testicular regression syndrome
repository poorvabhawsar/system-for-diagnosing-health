   <div id="recipe_finder" class="recipe_finder_rdr"> 
    <div class="toc_promo"> 
     <h3>WebMD Recipe Finder</h3> 
     <p>Browse our collection of healthy, delicious recipes, from WebMD and <em>Eating Well</em> magazine.</p> 
     <input id="txtFreeSearch" type="text" title="Enter a keyword or phrase" /> 
     <br />
     <br />
     <input type="image" alt="Search" src="http://img.webmd.com/dtmcms/live/webmd/consumer_assets/site_images/tools/recipe-finder/btn_search.gif" onclick="sendToTOC()" /> 
    </div> 
   </div> 
   <script>
function sendToTOC()
{
	//get the text value and strip the special chars
	var search_string = $('#txtFreeSearch').val();

        search_string = search_string.replace("#",""); //firefox fix

        search_string = encodeURIComponent(search_string);
	
	//put in the script to pass value to the toc
	window.location = '/food-recipes/healthy-recipe-finder#f=' + search_string + '&s=0&m=1&o=srch-rcp_rr'
	
}

//put the script to create a prompt in the text box
$(document).ready(function()
{
  $('input[type=text][title]').each(function(i){
    $(this).addClass('input-prompt-' + i);
    var promptSpan = $('<span class="input-prompt"/>');
    $(promptSpan).attr('id', 'input-prompt-' + i);
    $(promptSpan).append($(this).attr('title'));
    $(promptSpan).click(function(){
      $(this).hide();
      $('.' + $(this).attr('id')).focus();
    });
    if($(this).val() != ''){
      $(promptSpan).hide();
    }
    $(this).before(promptSpan);
    $(this).focus(function(){
      $('#input-prompt-' + i).hide();
    });
    $(this).blur(function()
    {
      if($(this).val() == '')
      {
        $('#input-prompt-' + i).show();
      }
    });
  });
});

$(document).ready(function() 
{
  //set the enter key
  $("#txtFreeSearch").keyup(function(event){ 
    if(event.keyCode == 13){ 
       sendToTOC(); 
    } 
  }); 
  
})

</script>
  </div> 
