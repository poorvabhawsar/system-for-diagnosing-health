				"\"Best Life\" Magazine",
				"\"Cure\" Magazine",
				"WebMD Veterinary Reference from the ASPCA",
				"WebMD Veterinary Reference from ASPCA Virtual Pet Behaviorist",
				"COPD Foundation"
				]

				/* Array of names that go with files in partners node */
				var partnerScripts=[
				"goodHousekeeping",
				"redbook",
				"esquire",
				"countryLiving",
				"marieClaire",
				"cookingLight",
				"health",
				"southernLiving",
				"psychToday",
				"eatingWell",
				"drGreene",
