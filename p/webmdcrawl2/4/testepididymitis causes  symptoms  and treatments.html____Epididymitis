></target></object><object chronic_id="091e9c5e8001cd3e" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/special/epididymitis-references_special_tv7769spec-bib.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/men/tc/epididymitis-references"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/men/tc/epididymitis-references"></target></object><object chronic_id="091e9c5e8001cfdc" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/special/epididymitis-credits_special_tv7769spec-credits.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/men/tc/epididymitis-credits"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/men/tc/epididymitis-credits"></target></object></referenced_objects><container_hierarchy></container_hierarchy><topic_outline chronic_id="091e9c5e800236f6" path="/webmd/consumer_assets/controlled_content/healthwise/toc/special/epididymitis_special_tv7769spec.wxml"></topic_outline><expanded_objects></expanded_objects><cons_topic_outline><section_groups><section_group><section_title>Topic Overview</section_title><section_type>SpecialSection</section_type><section_link directive="article" chronic_id="091e9c5e8001cd98" path="/webmd/consumer_assets/controlled_content/healthwise/special/epididymitis-topic_overview_special_tv7770.xml" object_type="wbmd_cons_article"></section_link></section_group><section_group><section_title>Other Places To Get Help</section_title><section_type>ConsumerResource</section_type><section_link directive="article" chronic_id="091e9c5e807f0e00" path="/webmd/consumer_assets/controlled_content/healthwise/special/epididymitis-other_places_to_get_help_special_tv7769spec-conres.xml" object_type="wbmd_cons_article"></section_link></section_group><section_group><section_title>Related Information</section_title><section_type>RelatedInformation</section_type><section_link directive="article" chronic_id="091e9c