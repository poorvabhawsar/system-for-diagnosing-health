8" name="WebMD_Mobile" prefix="www" friendlyurl="/oral-health/tc/voice-problems-related-information"></target></object><object chronic_id="091e9c5e8001d076" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/special/voice_problems-references_special_ty7246-bib.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/oral-health/tc/voice-problems-references"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/oral-health/tc/voice-problems-references"></target></object><object chronic_id="091e9c5e8001ce33" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/special/voice_problems-credits_special_ty7246-credits.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/oral-health/tc/voice-problems-credits"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/oral-health/tc/voice-problems-credits"></target></object></referenced_objects><container_hierarchy></container_hierarchy><topic_outline chronic_id="091e9c5e800236ea" path="/webmd/consumer_assets/controlled_content/healthwise/toc/special/voice_problems_special_ty7246.wxml"></topic_outline><expanded_objects></expanded_objects><cons_topic_outline><section_groups><section_group><section_title>Overview</section_title><section_type>SpecialSection</section_type><section_link directive="article" chronic_id="091e9c5e8001d562" path="/webmd/consumer_assets/controlled_content/healthwise/special/voice_problems-overview_special_ty7247.xml" object_type="wbmd_cons_article"></section_link></section_group><section_group><section_title>Other Places To Get Help</section_title><section_type>ConsumerResource</section_type><section_link directive="article" chronic_id="091e9c5e804908d0" path="/webmd/consumer_assets/controlled_content/healthwise/special/voice_problems-other_places_to_get_help_special_ty7246-conres.xml" object_type="wbmd_cons_article"></section_link></section_group><section_group><section_title>Related Info