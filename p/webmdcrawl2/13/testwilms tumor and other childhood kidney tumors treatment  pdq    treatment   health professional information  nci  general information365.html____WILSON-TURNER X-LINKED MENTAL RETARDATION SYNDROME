			});
            
			topics.push({
			title : 'Treatment of Rhabdoid Tumor of the Kidney',
			href : 'http://www.webmd.com/cancer/tc/ncicdr0000062789-treatment-of-rhabdoid-tumor-of-the-kidney'
			});
            
			topics.push({
			title : 'Treatment of Neuroepithelial Tumor of the Kidney',
			href : 'http://www.webmd.com/cancer/tc/ncicdr0000062789-treatment-of-neuroepithelial-tumor-of-the-kidney'
			});
            
			topics.push({
			title : 'Treatment of Congenital Mesoblastic Nephroma',
			href : 'http://www.webmd.com/cancer/tc/ncicdr0000062789-treatment-of-congenital-mesoblastic-nephroma'
			});
            
			topics.push({
			title : 'Treatment of Renal Cell Carcinoma',
			href : 'http://www.webmd.com/cancer/tc/ncicdr0000062789-treatment-of-renal-cell-carcinoma'
			});
            
			topics.push({
			title : 'Treatment of Recurrent Wilms Tumor and Other Childhood Kidney Tumors',
			href : 'http://www.webmd.com/cancer/tc/ncicdr0000062789-treatment-of-recurrent-wilms-tumor-and-other-childhood-kidney-tumors'
			});
            
			topics.push({
			title : 'Changes to This Summary (11 / 27 / 2012)',
			href : 'http://www.webmd.com/cancer/tc/wilms-tumor-and-other-childhood-kidney-tumors-treatment-pdq-treatment---health-professional-information-nci-changes-to-this-summary-11--27--2012'
			});
            
			topics.push({
			title : 'About This PDQ Summary',
			href : 'http://www.webmd.com/cancer/tc/ncicdr0000062789-about-this-pdq-summary'
			});
            
			topics.push({
			title : 'Get More Information From NCI',
			href : 'http://www.webmd.com/cancer/tc/ncicdr0000062789-get-more-information-from-nci'
			});
            	
	
	/* set Current topic */	
	jQuery.each(topics, function(i, val) {
    if (window.location.href.indexOf(topics[i].href) > -1){
	   currentTopic = i;
	   }
    });
	

	/* if on the first article in a set, remove the previous navigation */
		if ( currentTopic == 0 ) {
			$(".pagination_fmt .left_fmt .outline_fmt").hide();
		}
		if ( window.location.href.indexOf("page=") > 1 ) {
			$(".pagination_fmt .left_fmt .outline_fmt").show();
		}

	
	/* if on the last article in a set, remove the next navigation */
	if ( (currentTopic + 1) == topics.length) {
			$(".pagination_fmt .right_fmt .outline_fmt").hide();
		
		}

/* Inserting Previous */
   if (currentTopic != 0){
   var a_hrefl = $('.pagination_fmt .left_fmt .outline_fmt a').attr('href');
   if (typeof(a_hrefl) === "undefined"){
   	$(".pagination_fmt .left_fmt .outline_fmt a").attr("href" , a_hrefl);
   } else {
    $(".pagination_fmt .left_fmt .outline_fmt a").attr("href" , topics[currentTopic - 1].href);
    }
    }


		
	/* Inserting Next */
   if ( (currentTopic + 1) != topics.length ) {
   var a_hrefr = $('.pagination_fmt .right_fmt .outline_fmt a').attr('href');
   if (typeof(a_hrefr) === "undefined"){
   $(".pagination_fmt .right_fmt .outline_fmt a").attr("href" , a_hrefr);
   }
   else if ( window.location.href.indexOf("check-your-symptoms") > -1 ) {
			$(".pagination_fmt .right_fmt .outline_fmt a").attr("href" , topics[currentTopic + 1].href);
		}
   else if (a_hrefr.indexOf('page') != -1 ) {}
   else {$(".pagination_fmt .right_fmt .outline_fmt a").attr("href" , topics[currentTopic + 1].href);}
	}
   
			
	/* Secondary pages with no data move to the next page.  */
			
	/* adding metrics to nav */
	$(".right_fmt a").on("click", function() {
		return sl(this,'','pagenum-art_next');
	});
	$(".left_fmt a").on("click", function() {
		return sl(this,'','pagenum-art_prev');
	});
	
});</script>
