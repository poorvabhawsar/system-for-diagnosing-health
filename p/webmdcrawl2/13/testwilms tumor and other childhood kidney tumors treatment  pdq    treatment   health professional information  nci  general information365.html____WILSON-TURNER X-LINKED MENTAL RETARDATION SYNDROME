   <script>$(function() {

	var currentPage = 1, ct = 0, currentTopic, a_hrefr, a_hrefl, pageParam, totalPages = 1, topics = [];
	
	/* set currentPage */
	pageParam = parseInt( webmd.url.getParam("page") );
	if ( pageParam ) {
		currentPage = pageParam;
	}
	
	/* set topics */
	
			topics.push({
			title : 'Nci',
			href : 'http://www.webmd.com/cancer/tc/wilms-tumor-and-other-childhood-kidney-tumors-treatment-pdq-treatment---health-professional-information-nci-nci_ncicdr0000062789-nci-header'
			});
            
			topics.push({
			title : 'General Information',
			href : 'http://www.webmd.com/cancer/tc/wilms-tumor-treatment-health-professional-information-nci-pdq-general-information'
			});
            
			topics.push({
			title : 'Cellular Classification',
