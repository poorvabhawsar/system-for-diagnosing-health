			href : 'http://www.webmd.com/heart-disease/tc/supraventricular-tachycardia-when-to-call-a-doctor'
			});
            
			topics.push({
			title : 'Exams and Tests',
			href : 'http://www.webmd.com/heart-disease/tc/supraventricular-tachycardia-exams-and-tests'
			});
            
			topics.push({
			title : 'Treatment Overview',
			href : 'http://www.webmd.com/heart-disease/tc/supraventricular-tachycardia-treatment-overview'
			});
            
			topics.push({
			title : 'Ongoing Concerns',
			href : 'http://www.webmd.com/heart-disease/tc/supraventricular-tachycardia-ongoing-concerns'
			});
            
			topics.push({
			title : 'Prevention',
			href : 'http://www.webmd.com/heart-disease/tc/supraventricular-tachycardia-prevention'
			});
            
			topics.push({
			title : 'Living With Tachycardia',
			href : 'http://www.webmd.com/heart-disease/tc/supraventricular-tachycardia-living-with-tachycardia'
			});
            
			topics.push({
			title : 'Medications',
			href : 'http://www.webmd.com/heart-disease/tc/supraventricular-tachycardia-medications'
			});
            
			topics.push({
			title : 'Surgery',
			href : 'http://www.webmd.com/heart-disease/tc/supraventricular-tachycardia-surgery'
			});
            
			topics.push({
			title : 'Other Treatment',
			href : 'http://www.webmd.com/heart-disease/tc/supraventricular-tachycardia-other-treatment'
			});
            
			topics.push({
			title : 'Other Places To Get Help',
			href : 'http://www.webmd.com/heart-disease/tc/supraventricular-tachycardia-other-places-to-get-help'
			});
            
			topics.push({
			title : 'Related Information',
			href : 'http://www.webmd.com/heart-disease/tc/supraventricular-tachycardia-related-information'
			});
            
			topics.push({
			title : 'References',
			href : 'http://www.webmd.com/heart-disease/tc/supraventricular-tachycardia-references'
			});
            
			topics.push({
			title : 'Credits',
			href : 'http://www.webmd.com/heart-disease/tc/supraventricular-tachycardia-credits'
			});
            	
	
	/* set Current topic */	
	jQuery.each(topics, function(i, val) {
    if (window.location.href.indexOf(topics[i].href) > -1){
	   currentTopic = i;
	   }
    });
	

	/* if on the first article in a set, remove the previous navigation */
		if ( currentTopic == 0 ) {
			$(".pagination_fmt .left_fmt .outline_fmt").hide();
		}
		if ( window.location.href.indexOf("page=") > 1 ) {
			$(".pagination_fmt .left_fmt .outline_fmt").show();
		}

	
	/* if on the last article in a set, remove the next navigation */
	if ( (currentTopic + 1) == topics.length) {
			$(".pagination_fmt .right_fmt .outline_fmt").hide();
		
		}

/* Inserting Previous */
   if (currentTopic != 0){
   var a_hrefl = $('.pagination_fmt .left_fmt .outline_fmt a').attr('href');
   if (typeof(a_hrefl) === "undefined"){
   	$(".pagination_fmt .left_fmt .outline_fmt a").attr("href" , a_hrefl);
   } else {
    $(".pagination_fmt .left_fmt .outline_fmt a").attr("href" , topics[currentTopic - 1].href);
    }
    }


		
	/* Inserting Next */
   if ( (currentTopic + 1) != topics.length ) {
   var a_hrefr = $('.pagination_fmt .right_fmt .outline_fmt a').attr('href');
   if (typeof(a_hrefr) === "undefined"){
   $(".pagination_fmt .right_fmt .outline_fmt a").attr("href" , a_hrefr);
   }
   else if ( window.location.href.indexOf("check-your-symptoms") > -1 ) {
			$(".pagination_fmt .right_fmt .outline_fmt a").attr("href" , topics[currentTopic + 1].href);
		}
   else if (a_hrefr.indexOf('page') != -1 ) {}
   else {$(".pagination_fmt .right_fmt .outline_fmt a").attr("href" , topics[currentTopic + 1].href);}
	}
   
			
	/* Secondary pages with no data move to the next page.  */
			
	/* adding metrics to nav */
	$(".right_fmt a").on("click", function() {
		return sl(this,'','pagenum-art_next');
	});
	$(".left_fmt a").on("click", function() {
		return sl(this,'','pagenum-art_prev');
	});
	
});</script>
