			}

			function checkPartners(){

				/* Array of Partner Names */
				var partnerNames=[
				"Good Housekeeping",
				"Redbook",
				"Esquire",
				"Country Living",
				"Marie Claire",
				"Oxmoor House",
				"\"Health",
				"Southern Living",
				"Psychology Today",
				"EatingWell",
				"DrGreene.com",
				"Men's Health",
				"Women's Health",
				"Prevention",
				"eDiets",
				"soul\" Magazine",
				"Runner's World",
				"Men's Fitness\" Magazine",
				"Shape\" Magazine",
				"Fit Pregnancy\" Magazine",
				"Natural Health\" Magazine",
				"\"Best Life\" Magazine",
				"\"Cure\" Magazine",
				"WebMD Veterinary Reference from the ASPCA",
				"WebMD Veterinary Reference from ASPCA Virtual Pet Behaviorist",
				"COPD Foundation"
				]

