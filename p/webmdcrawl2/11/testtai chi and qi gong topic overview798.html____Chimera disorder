ences"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/fitness-exercise/tc/tai-chi-and-qi-gong-references"></target></object><object chronic_id="091e9c5e8001d694" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/special/tai_chi_and_qi_gong-credits_special_aa106255spec-credits.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/fitness-exercise/tc/tai-chi-and-qi-gong-credits"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/fitness-exercise/tc/tai-chi-and-qi-gong-credits"></target></object></referenced_objects><container_hierarchy></container_hierarchy><topic_outline chronic_id="091e9c5e800237a8" path="/webmd/consumer_assets/controlled_content/healthwise/toc/special/tai_chi_and_qi_gong_special_aa106255spec.wxml"></topic_outline><expanded_objects></expanded_objects><cons_topic_outline><section_groups><section_group><section_title>Topic Overview</section_title><section_type>SpecialSection</section_type><section_link directive="article" chronic_id="091e9c5e8001d37b" path="/webmd/consumer_assets/controlled_content/healthwise/special/tai_chi_and_qi_gong-topic_overview_special_tp21251.xml" object_type="wbmd_cons_article"></section_link></section_group><section_group><section_title>Related Information</section_title><section_type>RelatedInformation</section_type><section_link directive="article" chronic_id="091e9c5e8001d1b0" path="/webmd/consumer_assets/controlled_content/healthwise/special/tai_chi_and_qi_gong-related_information_special_aa106255spec-relinfo.xml" object_type="wbmd_cons_article"></section_link></section_group><section_group><section_title>References</section_title><section_type>References</section_type><section_link directive="article" chronic_id="091e9c5e8013f111" path="/webmd/consumer_assets/controlled_content/healthwise/special/tai_chi_and_qi_gong-references_special_aa106255spec-bib.xml" object_type="wbmd_cons_article"></section_link></section_group><section_