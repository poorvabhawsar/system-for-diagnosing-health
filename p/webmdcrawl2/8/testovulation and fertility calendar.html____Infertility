   <script>
(function(){

	var appid, dest, url;

	appid = '31';
	dest = 'https://opc.webmd.com/baby_calendars/ovulation_calendar.aspx?service=ov';

	// Check i user is signed in
	if(webmd.cookie.exists("WBMD_AUTH")){

		// Add a class that will hide some of the links and show others
		$('#ovulation_calender_promo').addClass("signed_in");

		// TEMPORARY FIX
		// Since calendar requires a screen name, and some accounts might not have one,
		// Check if user is missing screenname and redirect through additional info page if necessary
		if (!window.nickname || window.nickname.indexOf('@') != -1) {
			url = 'https://member' + webmd.url.getEnv() + '.webmd.com/additional-info';
			url = webmd.url.setParam('appid', appid, url);
			url = webmd.url.setParam('returl', dest, url);
			$('#view-my-calendar').attr('href', url);
		}
	}
})();
</script> 
