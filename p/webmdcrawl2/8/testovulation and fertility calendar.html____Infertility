            // self executing function for scope
                (function(){

                  // grabs pageview id out of global scope and makes sure it exists as we need to pass it to ads in that case
                  var pageviewId = window.s_pageview_id || '';

                  var iframeURLOutOfPB = '//as.webmd.com/html.ng/transactionID=164873082&tile=2067694258&tug=&pug=__&site=2&affiliate=20&hcent=793&scent=&pos=121&xpg=3607&sec=&amp;au1=&au2=&uri=%2fbaby%2fhealthtool-ovulation-calendar&artid=091e9c5e801f81cc&inst=0&amp;leaf=2047&segm=0&cc=100&tmg=&bc=__&mcent=&micro=&pvid=' + pageviewId;

                  var cleanIframeURL = iframeURLOutOfPB.replace(/&amp;/g, '&');

                  var ad = {
                    adLocation:'right',
                    adURL:cleanIframeURL,  
                    trans:'164873082',
                    tile:'2067694258',
                    pos:'121'
                  };

                  // check to make sure this seed call functionality exists, if it does, dooo it
