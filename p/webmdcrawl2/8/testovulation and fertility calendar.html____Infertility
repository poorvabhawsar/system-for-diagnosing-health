    var capps_server = 'https://opc.webmd.com';
    function Ovul_Window(){
        window.open(capps_server + "/baby_calendars/ovulation_calendar.aspx?service=ov", "ovul_window", "toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbars=1,resizable=1,copyhistory=0,width=640,height=526");
    }
    
    function Ovul_join_Window(){
        var customRegisterURL = 'https://member.webmd.com/register.aspx?appid=2&returl=' + capps_server + '/baby_calendars/ovulation_calendar.aspx?service=ov';
        window.open(customRegisterURL, "dfj_window", "toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbars=1,resizable=1,copyhistory=0,width=640,height=526");
    }
</script>
   <div id="ovulation_calender_promo"> 
    <div id="ovulation_cal_top"> 
     <h2>WebMD Ovulation Calendar</h2> 
     <p>Want to boost your chances of getting pregnant? Tracking your menstrual<br />cycle is a simple way to predict when you’re most likely to conceive.</p> 
     <p><strong>Our Ovulation Calendar tells you:</strong></p> 
     <ul> 
      <li>Which days you’re most fertile.</li> 
      <li>The earliest day you can effectively use<br />a home pregnancy test.</li> 
      <li>What you can do to increase the odds of<br />conceiving a girl.</li> 
      <li>How to improve your chances of having a boy.</li> 
     </ul> 
    </div> 
    <div id="ovulation_cal_btm"> 
