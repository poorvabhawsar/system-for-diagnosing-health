   <script>$(function() {

	var currentPage = 1, ct = 0, currentTopic, a_hrefr, a_hrefl, pageParam, totalPages = 1, topics = [];
	
	/* set currentPage */
	pageParam = parseInt( webmd.url.getParam("page") );
	if ( pageParam ) {
		currentPage = pageParam;
	}
	
	/* set topics */
	
			topics.push({
			title : 'Nci',
			href : 'http://www.webmd.com/lung-cancer/tc/non-small-cell-lung-cancer-treatment-pdq-treatment---patient-information-nci-nci_ncicdr0000062956-nci-header'
			});
            
			topics.push({
			title : 'General Information About Non-Small Cell Lung Cancer',
			href : 'http://www.webmd.com/lung-cancer/tc/lung-cancer-non-small-cell-treatment-patient-information-nci-pdq-general-information-about'
			});
            
			topics.push({
			title : 'Stages of Non-Small Cell Lung Cancer',
			href : 'http://www.webmd.com/lung-cancer/tc/lung-cancer-non-small-cell-treatment-patient-information-nci-pdq-stages-of-non-small-cell-lung'
			});
            
			topics.push({
			title : 'Recurrent Non-Small Cell Lung Cancer',
			href : 'http://www.webmd.com/lung-cancer/tc/lung-cancer-non-small-cell-treatment-patient-information-nci-pdq-recurrent-non-small-cell-lung'
			});
            
			topics.push({
			title : 'Treatment Option Overview',
			href : 'http://www.webmd.com/lung-cancer/tc/lung-cancer-non-small-cell-treatment-patient-information-nci-pdq-treatment-option-overview'
			});
            
			topics.push({
			title : 'Treatment Options by Stage',
			href : 'http://www.webmd.com/lung-cancer/tc/lung-cancer-non-small-cell-treatment-patient-information-nci-pdq-treatment-options-by-stage'
			});
            
			topics.push({
			title : 'Treatment Options for Recurrent Non-Small Cell Lung Cancer',
			href : 'http://www.webmd.com/lung-cancer/tc/lung-cancer-non-small-cell-treatment-patient-information-nci-pdq-treatment-options-for-recurrent'
			});
            
			topics.push({
			title : 'To Learn More About Non-Small Cell Lung Cancer',
			href : 'http://www.webmd.com/lung-cancer/tc/ncicdr0000062956-to-learn-more-about-non-small-cell-lung-cancer'
			});
            
			topics.push({
			title : 'Changes to This Summary (03 / 20 / 2013)',
			href : 'http://www.webmd.com/lung-cancer/tc/non-small-cell-lung-cancer-treatment-pdq-treatment---patient-information-nci-changes-to-this-summary-03--20--2013'
			});
            
			topics.push({
			title : 'Get More Information From NCI',
			href : 'http://www.webmd.com/lung-cancer/tc/ncicdr0000062956-get-more-information-from-nci'
			});
            
			topics.push({
			title : 'About PDQ',
			href : 'http://www.webmd.com/lung-cancer/tc/lung-cancer-non-small-cell-treatment-patient-information-nci-pdq-about-pdq'
			});
            	
	
	/* set Current topic */	
	jQuery.each(topics, function(i, val) {
    if (window.location.href.indexOf(topics[i].href) > -1){
	   currentTopic = i;
	   }
    });
	

	/* if on the first article in a set, remove the previous navigation */
		if ( currentTopic == 0 ) {
			$(".pagination_fmt .left_fmt .outline_fmt").hide();
		}
		if ( window.location.href.indexOf("page=") > 1 ) {
			$(".pagination_fmt .left_fmt .outline_fmt").show();
		}

	
	/* if on the last article in a set, remove the next navigation */
	if ( (currentTopic + 1) == topics.length) {
			$(".pagination_fmt .right_fmt .outline_fmt").hide();
		
		}

/* Inserting Previous */
   if (currentTopic != 0){
   var a_hrefl = $('.pagination_fmt .left_fmt .outline_fmt a').attr('href');
   if (typeof(a_hrefl) === "undefined"){
   	$(".pagination_fmt .left_fmt .outline_fmt a").attr("href" , a_hrefl);
   } else {
    $(".pagination_fmt .left_fmt .outline_fmt a").attr("href" , topics[currentTopic - 1].href);
    }
    }


		
	/* Inserting Next */
   if ( (currentTopic + 1) != topics.length ) {
   var a_hrefr = $('.pagination_fmt .right_fmt .outline_fmt a').attr('href');
   if (typeof(a_hrefr) === "undefined"){
   $(".pagination_fmt .right_fmt .outline_fmt a").attr("href" , a_hrefr);
   }
   else if ( window.location.href.indexOf("check-your-symptoms") > -1 ) {
			$(".pagination_fmt .right_fmt .outline_fmt a").attr("href" , topics[currentTopic + 1].href);
		}
   else if (a_hrefr.indexOf('page') != -1 ) {}
   else {$(".pagination_fmt .right_fmt .outline_fmt a").attr("href" , topics[currentTopic + 1].href);}
	}
   
			
	/* Secondary pages with no data move to the next page.  */
			
	/* adding metrics to nav */
	$(".right_fmt a").on("click", function() {
		return sl(this,'','pagenum-art_next');
	});
	$(".left_fmt a").on("click", function() {
		return sl(this,'','pagenum-art_prev');
	});
	
});</script>
