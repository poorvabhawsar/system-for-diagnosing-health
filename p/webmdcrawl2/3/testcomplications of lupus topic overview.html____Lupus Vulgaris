eurisy"></target></object><object chronic_id="091e9c5e8001b63f" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/definition/pulmonary_embolism_definition_stp2035.wxml"><target siteid="3" name="" prefix="www" friendlyurl="/hw-popup/pulmonary-embolism"></target><target siteid="8" name="" prefix="www" friendlyurl="/hw-popup/pulmonary-embolism"></target></object><object chronic_id="091e9c5e8001b1f4" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/definition/pulmonary_edema_definition_stp1914.wxml"><target siteid="3" name="" prefix="www" friendlyurl="/hw-popup/pulmonary-edema"></target><target siteid="8" name="" prefix="www" friendlyurl="/hw-popup/pulmonary-edema"></target></object><object chronic_id="091e9c5e8001b66a" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/definition/hemolytic_anemia_definition_sth149813.wxml"><target siteid="3" name="" prefix="www" friendlyurl="/hw-popup/hemolytic-anemia"></target><target siteid="8" name="" prefix="www" friendlyurl="/hw-popup/hemolytic-anemia"></target></object><object chronic_id="091e9c5e8001b3f3" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/definition/thrombocytopenia_definition_stt11273.wxml"><target siteid="3" name="" prefix="www" friendlyurl="/hw-popup/thrombocytopenia"></target><target siteid="8" name="" prefix="www" friendlyurl="/hw-popup/thrombocytopenia"></target></object><object chronic_id="091e9c5e8001b255" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/definition/spleen_definition_sts15103.wxml"><target siteid="3" name="" prefix="www" friendlyurl="/hw-popup/spleen-7445"></target><target siteid="8" name="" prefix="www" friendlyurl="/hw-popup/spleen-7445"></target></object><object chronic_id="091e9c5e8001b5ec" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/definition/lymph_nodes_definition_stl158216.wxml"><target sitei