ion_title><section_type>ExamsAndTests</section_type><section_link directive="article" chronic_id="091e9c5e8001aede" path="/webmd/consumer_assets/controlled_content/healthwise/mini/Cushings_Syndrome-Exams_and_Tests.xml" object_type="wbmd_cons_article"></section_link></section_group><section_group><section_title>Treatment Overview</section_title><section_type>TreatmentOverview</section_type><section_link directive="article" chronic_id="091e9c5e8001aec6" path="/webmd/consumer_assets/controlled_content/healthwise/mini/Cushings_Syndrome-Treatment_Overview.xml" object_type="wbmd_cons_article"></section_link></section_group><section_group><section_title>Home Treatment</section_title><section_type>HomeTreatment</section_type><section_link directive="article" chronic_id="091e9c5e8001ae1b" path="/webmd/consumer_assets/controlled_content/healthwise/mini/Cushings_Syndrome-Home_Treatment.xml" object_type="wbmd_cons_article"></section_link></section_group><section_group><section_title>Other Places To Get Help</section_title><section_type>ConsumerResource</section_type><section_link directive="article" chronic_id="091e9c5e8001ad72" path="/webmd/consumer_assets/controlled_content/healthwise/mini/Cushings_Syndrome-Other_Places_To_Get_Help.xml" object_type="wbmd_cons_article"></section_link></section_group><section_group><section_title>Related Information</section_title><section_type>RelatedInformation</section_type><section_link directive="article" chronic_id="091e9c5e8001ae56" path="/webmd/consumer_assets/controlled_content/healthwise/mini/Cushings_Syndrome-Related_Information.xml" object_type="wbmd_cons_article"></section_link></section_group><section_group><section_title>References</section_title><section_type>References</section_type><section_link directive="article" chronic_id="091e9c5e8001aad0" path="/webmd/consumer_assets/controlled_content/healthwise/mini/Cushings_Syndrome-References.xml" object_type="wbmd_cons_article"></section_link></section_group><section_group><section_