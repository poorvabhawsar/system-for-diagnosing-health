l"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/cold-and-flu/tc/hand-washing-topic-overview"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/cold-and-flu/tc/hand-washing-topic-overview"></target></object><object chronic_id="091e9c5e808f66c6" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/special/handling_a_dead_bird-topic_overview_special_zd1215-sec.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/a-to-z-guides/handling-a-dead-bird-topic-overview"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/a-to-z-guides/tc/handling-a-dead-bird-topic-overview"></target></object><object chronic_id="091e9c5e808f66c3" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/special/handling_wild_birds-topic_overview_special_zd1216-sec.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/a-to-z-guides/handling-wild-birds-topic-overview"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/a-to-z-guides/tc/handling-wild-birds-topic-overview"></target></object><object chronic_id="091e9c5e8001d4c0" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/special/avian_influenza_bird_flu-overview_special_tp23639.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/cold-and-flu/tc/avian-influenza-bird-flu-overview"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/cold-and-flu/tc/avian-influenza-bird-flu-overview"></target></object><object chronic_id="091e9c5e807f0ef2" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/special/avian_influenza_bird_flu-other_places_to_get_help_special_tp23638spec-conres.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/cold-and-flu/tc/avian-influenza-bird-flu-other-places-to-get-help"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/cold-and-flu/tc/avia