/consumer_assets/controlled_content/healthwise/toc/special/rectal_prolapse_special_hw181291.wxml"></topic_outline><expanded_objects></expanded_objects><cons_topic_outline><section_groups><section_group><section_title>Topic Overview</section_title><section_type>SpecialSection</section_type><section_link directive="article" chronic_id="091e9c5e807f143c" path="/webmd/consumer_assets/controlled_content/healthwise/special/rectal_prolapse-topic_overview_special_hw181293.xml" object_type="wbmd_cons_article"></section_link></section_group><section_group><section_title>Frequently Asked Questions</section_title><section_type>SpecialSection</section_type><section_link directive="article" chronic_id="091e9c5e807f143d" path="/webmd/consumer_assets/controlled_content/healthwise/special/rectal_prolapse-frequently_asked_questions_special_abk7249.xml" object_type="wbmd_cons_article"></section_link></section_group><section_group><section_title>Other Places To Get Help</section_title><section_type>ConsumerResource</section_type><section_link directive="article" chronic_id="091e9c5e807f143e" path="/webmd/consumer_assets/controlled_content/healthwise/special/rectal_prolapse-other_places_to_get_help_special_hw181291-conres.xml" object_type="wbmd_cons_article"></section_link></section_group><section_group><section_title>Related Information</section_title><section_type>RelatedInformation</section_type><section_link directive="article" chronic_id="091e9c5e807f143f" path="/webmd/consumer_assets/controlled_content/healthwise/special/rectal_prolapse-related_information_special_hw181291-relinfo.xml" object_type="wbmd_cons_article"></section_link></section_group><section_group><section_title>References</section_title><section_type>References</section_type><section_link directive="article" chronic_id="091e9c5e807f1440" path="/webmd/consumer_assets/controlled_content/healthwise/special/rectal_prolapse-references_special_hw181291-bib.xml" object_type="wbmd_cons_article"></section_link></section_group><section_group><section_title>Credits</section_title><section_type>Credits</section_type><section_link directive="article" chronic_id="091e9c5e807f1441" path="/webmd/consumer_assets/controlled_content/healthwise/special/rectal_prolapse-credits_special_hw181291-credits.xml" object_type="wbmd_cons_article"></section_link></section_group></section_groups></cons_topic_outline><title>Rectal Prolapse</title><dynamic_objects><callout_title>Recommended Related to Health A-Z</callout_title><wbmd_asset object_type="wbmd_cons_article" id="091e9c5e80e929b0"><metadata_section><i_chronicle_id>091e9c5e807f09ba</i_chronicle_id><title>Ferroportin Disease</title><wbmd_c_art_thmbnl></wbmd_c_art_thmbnl></metadata_section><section_text><p>
  Important
