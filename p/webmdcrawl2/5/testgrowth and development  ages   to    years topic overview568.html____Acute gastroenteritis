   <script>$(function() {

	var currentPage = 1, ct = 0, currentTopic, a_hrefr, a_hrefl, pageParam, totalPages = 1, topics = [];
	
	/* set currentPage */
	pageParam = parseInt( webmd.url.getParam("page") );
	if ( pageParam ) {
		currentPage = pageParam;
	}
	
	/* set topics */
	
			topics.push({
			title : 'Topic Overview',
			href : 'http://www.webmd.com/children/tc/growth-and-development-ages-6-to-10-years-topic-overview'
			});
            
			topics.push({
			title : 'Health Tools',
			href : 'http://www.webmd.com/children/tc/growth-and-development-ages-6-to-10-years-health-tools'
			});
            
			topics.push({
			title : 'What to Expect',
			href : 'http://www.webmd.com/children/tc/growth-and-development-ages-6-to-10-years-what-to-expect'
			});
            
			topics.push({
			title : 'Common Concerns',
			href : 'http://www.webmd.com/children/tc/growth-and-development-ages-6-to-10-years-common-concerns'
			});
            
			topics.push({
			title : 'Promoting Healthy Growth and Development',
			href : 'http://www.webmd.com/children/tc/growth-and-development-ages-6-to-10-years-promoting-healthy-growth-and-development'
			});
            
			topics.push({
			title : 'When to Call a Doctor',
			href : 'http://www.webmd.com/children/tc/growth-and-development-ages-6-to-10-years-when-to-call-a-doctor'
			});
            
			topics.push({
			title : 'Routine Checkups',
			href : 'http://www.webmd.com/children/guide/growth-and-development-ages-6-to-10-years-routine-checkups'
			});
            
			topics.push({
			title : 'Other Places To Get Help',
			href : 'http://www.webmd.com/children/tc/growth-and-development-ages-6-to-10-years-other-places-to-get-help'
			});
            
			topics.push({
			title : 'Related Information',
			href : 'http://www.webmd.com/children/tc/growth-and-development-ages-6-to-10-years-related-information'
			});
            
			topics.push({
			title : 'References',
			href : 'http://www.webmd.com/children/tc/growth-and-development-ages-6-to-10-years-references'
			});
            
			topics.push({
			title : 'Credits',
			href : 'http://www.webmd.com/children/tc/growth-and-development-ages-6-to-10-years-credits'
			});
            	
	
	/* set Current topic */	
	jQuery.each(topics, function(i, val) {
    if (window.location.href.indexOf(topics[i].href) > -1){
	   currentTopic = i;
	   }
    });
	

	/* if on the first article in a set, remove the previous navigation */
		if ( currentTopic == 0 ) {
			$(".pagination_fmt .left_fmt .outline_fmt").hide();
		}
		if ( window.location.href.indexOf("page=") > 1 ) {
			$(".pagination_fmt .left_fmt .outline_fmt").show();
		}

	
	/* if on the last article in a set, remove the next navigation */
	if ( (currentTopic + 1) == topics.length) {
			$(".pagination_fmt .right_fmt .outline_fmt").hide();
		
		}

/* Inserting Previous */
   if (currentTopic != 0){
   var a_hrefl = $('.pagination_fmt .left_fmt .outline_fmt a').attr('href');
   if (typeof(a_hrefl) === "undefined"){
   	$(".pagination_fmt .left_fmt .outline_fmt a").attr("href" , a_hrefl);
   } else {
    $(".pagination_fmt .left_fmt .outline_fmt a").attr("href" , topics[currentTopic - 1].href);
    }
    }


		
	/* Inserting Next */
   if ( (currentTopic + 1) != topics.length ) {
   var a_hrefr = $('.pagination_fmt .right_fmt .outline_fmt a').attr('href');
   if (typeof(a_hrefr) === "undefined"){
   $(".pagination_fmt .right_fmt .outline_fmt a").attr("href" , a_hrefr);
   }
   else if ( window.location.href.indexOf("check-your-symptoms") > -1 ) {
			$(".pagination_fmt .right_fmt .outline_fmt a").attr("href" , topics[currentTopic + 1].href);
		}
   else if (a_hrefr.indexOf('page') != -1 ) {}
   else {$(".pagination_fmt .right_fmt .outline_fmt a").attr("href" , topics[currentTopic + 1].href);}
	}
   
			
	/* Secondary pages with no data move to the next page.  */
			
	/* adding metrics to nav */
	$(".right_fmt a").on("click", function() {
		return sl(this,'','pagenum-art_next');
	});
	$(".left_fmt a").on("click", function() {
		return sl(this,'','pagenum-art_prev');
	});
	
});</script>
