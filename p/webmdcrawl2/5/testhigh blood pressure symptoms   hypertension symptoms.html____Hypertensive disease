					var moduleCenter = "ContentPane8";
					var moduleRight = "ContentPane25";

					addLoadEvent(function() {
							$('#'+moduleCenter).append(writePartnerCenter());
							$('#'+moduleRight).prepend(writePartnerRight());
					})
				}
			}

			checkPartners();

