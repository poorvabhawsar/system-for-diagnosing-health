avis"></target><target siteid="8" name="" prefix="www" friendlyurl="/jeanie-lerche-davis"></target></object><object chronic_id="091e9c5e800148e1" thmbnl="" pointer="1"><target friendlyurl="http://www.webmd.com/content/article/46/1826_50692.htm"></target></object><object chronic_id="091e9c5e80016635" thmbnl="" pointer="1"><target friendlyurl="/click?url=http%3a%2f%2fwww.aan.com%2fpublic%2findex.cfm"></target></object><object chronic_id="091e9c5e8001663f" thmbnl="" pointer="1"><target friendlyurl="/click?url=http%3a%2f%2fwww.thebrainmatters.org%2findex.cfm"></target></object><object chronic_id="091e9c5e80016647" thmbnl="" pointer="1"><target friendlyurl="/click?url=http%3a%2f%2fwww.achenet.org%2f"></target></object><object chronic_id="091e9c5e80016651" thmbnl="" pointer="1"><target friendlyurl="/click?url=http%3a%2f%2fwww.headaches.org%2f"></target></object></referenced_objects><container_hierarchy></container_hierarchy><expanded_objects></expanded_objects></webmd_rendition></webmd_rendition>
