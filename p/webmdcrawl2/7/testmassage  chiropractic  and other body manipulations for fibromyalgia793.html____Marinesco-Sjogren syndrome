6" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/definition/fibromyalgia_definition_stf124066.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/hw-popup/fibromyalgia"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/hw-popup/fibromyalgia"></target></object><object chronic_id="091e9c5e8001ceba" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/special/bodywork_and_manual_therapy-topic_overview_special_tp21269.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/fibromyalgia/tc/bodywork-or-manual-therapy-topic-overview"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/a-to-z-guides/tc/bodywork-or-manual-therapy-topic-overview"></target></object><object chronic_id="091e9c5e8001ce2d" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/special/bodywork_and_manual_therapy-related_information_special_aa104339spec-relinfo.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/fibromyalgia/tc/bodywork-or-manual-therapy-related-information"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/a-to-z-guides/tc/bodywork-or-manual-therapy-related-information"></target></object><object chronic_id="091e9c5e80e6c4da" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/special/bodywork_and_manual_therapy-references_special_aa104339spec-bib.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/fibromyalgia/tc/bodywork-and-manual-therapy-references"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/a-to-z-guides/tc/bodywork-and-manual-therapy-references"></target></object><object chronic_id="091e9c5e8001d357" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/special/bodywork_and_manual_therapy-credits_special_aa104339spec-credits.wxml"><target siteid="3" name="WebMD" prefix="www" friendly