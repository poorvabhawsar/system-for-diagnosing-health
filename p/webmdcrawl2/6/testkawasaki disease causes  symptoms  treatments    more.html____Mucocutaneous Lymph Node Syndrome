ck_definition_sth149627.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/hw-popup/heart-attack"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/hw-popup/heart-attack"></target></object><object chronic_id="091e9c5e807f1442" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/special/kawasaki_disease-topic_overview_special_hw180462.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/heart-disease/tc/kawasaki-disease-topic-overview"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/heart-disease/tc/kawasaki-disease-topic-overview"></target></object><object chronic_id="091e9c5e807f1443" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/special/kawasaki_disease-frequently_asked_questions_special_abk4215.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/heart-disease/tc/kawasaki-disease-frequently-asked-questions"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/heart-disease/tc/kawasaki-disease-frequently-asked-questions"></target></object><object chronic_id="091e9c5e807f1444" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/special/kawasaki_disease-other_places_to_get_help_special_hw180460-conres.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/heart-disease/tc/kawasaki-disease-other-places-to-get-help"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/heart-disease/tc/kawasaki-disease-other-places-to-get-help"></target></object><object chronic_id="091e9c5e807f1445" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/special/kawasaki_disease-related_information_special_hw180460-relinfo.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/heart-disease/tc/kawasaki-disease-related-information"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/heart-disease/