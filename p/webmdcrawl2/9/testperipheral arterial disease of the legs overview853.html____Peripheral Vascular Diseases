></object><object chronic_id="091e9c5e80012609" thmbnl="" pointer="0" path="/webmd/consumer_assets/editorial/articles/health_and_medical_reference/heart_and_blood_vessels/heart_disease/overview_types_of/heart_disease_heart_attacks.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/heart-disease/guide/heart-disease-heart-attacks"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/heart-disease/guide/heart-disease-heart-attacks"></target></object><object chronic_id="091e9c5e80025270" thmbnl="" pointer="0" path="/webmd/PageBuilder_Assets/scopemaps/WebMD Consumer/Pages/Stroke Health Center/page_Stroke Health Center_091e9c5e80025270.wxml"><target siteid="3" name="" prefix="www" friendlyurl="/stroke/default.htm"></target></object><object chronic_id="091e9c5e80bd1618" thmbnl="" pointer="0" path="/webmd/PageBuilder_Assets/scopemaps/WebMD Consumer/Pages/Diet future toc_091e9c5e80bd1618/page_Diet future toc_091e9c5e80bd1618.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/diet/default.htm" umtchronicleid="091e9c5e8068a387"></target><target siteid="8" name="" prefix="www" friendlyurl="/diet/default.htm" umtchronicleid="091e9c5e8068a387"></target></object><object chronic_id="091e9c5e8001d084" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/special/weight_management-are_you_at_a_healthy_weight_special_aa126305.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/diet/tc/healthy-weight-what-is-a-healthy-weight"></target><target siteid="8" name="WebMD_Mobile" prefix="www" friendlyurl="/diet/tc/healthy-weight-what-is-a-healthy-weight"></target></object><object chronic_id="091e9c5e8001b5f2" thmbnl="" pointer="0" path="/webmd/consumer_assets/controlled_content/healthwise/definition/cardiac_rehabilitation_definition_stc123631.wxml"><target siteid="3" name="WebMD" prefix="www" friendlyurl="/hw-popup/cardiac-rehabilitation"></target><target siteid="8" name="WebMD_Mobile" prefix="www" frien