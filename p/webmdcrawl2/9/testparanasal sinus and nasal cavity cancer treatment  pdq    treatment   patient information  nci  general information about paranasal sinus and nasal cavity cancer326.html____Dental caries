   <script>$(function() {

	var currentPage = 1, ct = 0, currentTopic, a_hrefr, a_hrefl, pageParam, totalPages = 1, topics = [];
	
	/* set currentPage */
	pageParam = parseInt( webmd.url.getParam("page") );
	if ( pageParam ) {
		currentPage = pageParam;
	}
	
	/* set topics */
	
			topics.push({
			title : 'Nci',
			href : 'http://www.webmd.com/cancer/tc/paranasal-sinus-and-nasal-cavity-cancer-treatment-pdq-treatment---patient-information-nci-nci_ncicdr0000258028-nci-header'
			});
            
			topics.push({
			title : 'General Information About Paranasal Sinus and Nasal Cavity Cancer',
			href : 'http://www.webmd.com/cancer/tc/ncicdr0000258028-general-information-about-paranasal-sinus-and-nasal-cavity-cancer'
			});
            
			topics.push({
			title : 'Stages of Paranasal Sinus and Nasal Cavity Cancer',
			href : 'http://www.webmd.com/cancer/tc/ncicdr0000258028-stages-of-paranasal-sinus-and-nasal-cavity-cancer'
			});
            
			topics.push({
			title : 'Recurrent Paranasal Sinus and Nasal Cavity Cancer',
			href : 'http://www.webmd.com/cancer/tc/ncicdr0000258028-recurrent-paranasal-sinus-and-nasal-cavity-cancer'
			});
            
			topics.push({
			title : 'Treatment Option Overview',
			href : 'http://www.webmd.com/cancer/tc/ncicdr0000258028-treatment-option-overview'
			});
            
			topics.push({
			title : 'Treatment Options by Stage',
			href : 'http://www.webmd.com/cancer/tc/ncicdr0000258028-treatment-options-by-stage'
			});
            
			topics.push({
			title : 'Treatment Options for Recurrent Paranasal Sinus and Nasal Cavity Cancer',
			href : 'http://www.webmd.com/cancer/tc/ncicdr0000258028-treatment-options-for-recurrent-paranasal-sinus-and-nasal-cavity-cancer'
			});
            
			topics.push({
			title : 'To Learn More About Paranasal Sinus and Nasal Cavity Cancer',
			href : 'http://www.webmd.com/cancer/tc/ncicdr0000258028-to-learn-more-about-paranasal-sinus-and-nasal-cavity-cancer'
			});
            
			topics.push({
			title : 'Changes to This Summary (01 / 18 / 2013)',
			href : 'http://www.webmd.com/cancer/tc/paranasal-sinus-and-nasal-cavity-cancer-treatment-pdq-treatment---patient-information-nci-changes-to-this-summary-01--18--2013'
			});
            
			topics.push({
			title : 'Get More Information From NCI',
			href : 'http://www.webmd.com/cancer/tc/ncicdr0000258028-get-more-information-from-nci'
			});
            
			topics.push({
			title : 'About PDQ',
			href : 'http://www.webmd.com/cancer/tc/ncicdr0000258028-about-pdq'
			});
            	
	
	/* set Current topic */	
	jQuery.each(topics, function(i, val) {
    if (window.location.href.indexOf(topics[i].href) > -1){
	   currentTopic = i;
	   }
    });
	

	/* if on the first article in a set, remove the previous navigation */
		if ( currentTopic == 0 ) {
			$(".pagination_fmt .left_fmt .outline_fmt").hide();
		}
		if ( window.location.href.indexOf("page=") > 1 ) {
			$(".pagination_fmt .left_fmt .outline_fmt").show();
		}

	
	/* if on the last article in a set, remove the next navigation */
	if ( (currentTopic + 1) == topics.length) {
			$(".pagination_fmt .right_fmt .outline_fmt").hide();
		
		}

/* Inserting Previous */
   if (currentTopic != 0){
   var a_hrefl = $('.pagination_fmt .left_fmt .outline_fmt a').attr('href');
   if (typeof(a_hrefl) === "undefined"){
   	$(".pagination_fmt .left_fmt .outline_fmt a").attr("href" , a_hrefl);
   } else {
    $(".pagination_fmt .left_fmt .outline_fmt a").attr("href" , topics[currentTopic - 1].href);
    }
    }


		
	/* Inserting Next */
   if ( (currentTopic + 1) != topics.length ) {
   var a_hrefr = $('.pagination_fmt .right_fmt .outline_fmt a').attr('href');
   if (typeof(a_hrefr) === "undefined"){
   $(".pagination_fmt .right_fmt .outline_fmt a").attr("href" , a_hrefr);
   }
   else if ( window.location.href.indexOf("check-your-symptoms") > -1 ) {
			$(".pagination_fmt .right_fmt .outline_fmt a").attr("href" , topics[currentTopic + 1].href);
		}
   else if (a_hrefr.indexOf('page') != -1 ) {}
   else {$(".pagination_fmt .right_fmt .outline_fmt a").attr("href" , topics[currentTopic + 1].href);}
	}
   
			
	/* Secondary pages with no data move to the next page.  */
			
	/* adding metrics to nav */
	$(".right_fmt a").on("click", function() {
		return sl(this,'','pagenum-art_next');
	});
	$(".left_fmt a").on("click", function() {
		return sl(this,'','pagenum-art_prev');
	});
	
});</script>
