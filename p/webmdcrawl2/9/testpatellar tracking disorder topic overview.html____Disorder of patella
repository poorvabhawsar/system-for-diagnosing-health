   <script>$(function() {

	var currentPage = 1, ct = 0, currentTopic, a_hrefr, a_hrefl, pageParam, totalPages = 1, topics = [];
	
	/* set currentPage */
	pageParam = parseInt( webmd.url.getParam("page") );
	if ( pageParam ) {
		currentPage = pageParam;
	}
	
	/* set topics */
	
			topics.push({
			title : 'Topic Overview',
			href : 'http://www.webmd.com/pain-management/knee-pain/tc/patellar-tracking-disorder-topic-overview'
			});
            
			topics.push({
			title : 'Health Tools',
			href : 'http://www.webmd.com/pain-management/knee-pain/tc/patellar-tracking-disorder-health-tools'
			});
            
			topics.push({
			title : 'Cause',
			href : 'http://www.webmd.com/pain-management/knee-pain/tc/patellar-tracking-disorder-cause'
			});
            
			topics.push({
			title : 'Symptoms',
			href : 'http://www.webmd.com/pain-management/knee-pain/tc/patellar-tracking-disorder-symptoms'
			});
            
			topics.push({
			title : 'What Happens',
			href : 'http://www.webmd.com/pain-management/knee-pain/tc/patellar-tracking-disorder-what-happens'
			});
            
			topics.push({
			title : 'What Increases Your Risk',
			href : 'http://www.webmd.com/pain-management/knee-pain/tc/patellar-tracking-disorder-what-increases-your-risk'
			});
            
			topics.push({
			title : 'When To Call a Doctor',
			href : 'http://www.webmd.com/pain-management/knee-pain/tc/patellar-tracking-disorder-when-to-call-a-doctor'
			});
            
			topics.push({
			title : 'Exams and Tests',
			href : 'http://www.webmd.com/pain-management/knee-pain/tc/patellar-tracking-disorder-exams-and-tests'
			});
            
			topics.push({
			title : 'Treatment Overview',
			href : 'http://www.webmd.com/pain-management/knee-pain/tc/patellar-tracking-disorder-treatment-overview'
			});
            
			topics.push({
			title : 'Prevention',
			href : 'http://www.webmd.com/pain-management/knee-pain/tc/patellar-tracking-disorder-prevention'
			});
            
			topics.push({
			title : 'Home Treatment',
			href : 'http://www.webmd.com/pain-management/knee-pain/tc/patellar-tracking-disorder-home-treatment'
			});
            
			topics.push({
			title : 'Medications',
			href : 'http://www.webmd.com/pain-management/knee-pain/tc/patellar-tracking-disorder-medications'
			});
            
			topics.push({
			title : 'Surgery',
			href : 'http://www.webmd.com/pain-management/knee-pain/tc/patellar-tracking-disorder-surgery'
			});
            
			topics.push({
			title : 'Other Treatment',
			href : 'http://www.webmd.com/pain-management/knee-pain/tc/patellar-tracking-disorder-other-treatment'
			});
            
			topics.push({
			title : 'Other Places To Get Help',
			href : 'http://www.webmd.com/pain-management/knee-pain/tc/patellar-tracking-disorder-other-places-to-get-help'
			});
            
			topics.push({
			title : 'Related Information',
			href : 'http://www.webmd.com/pain-management/knee-pain/tc/patellar-tracking-disorder-related-information'
			});
            
			topics.push({
			title : 'References',
			href : 'http://www.webmd.com/pain-management/knee-pain/tc/patellar-tracking-disorder-references'
			});
            
			topics.push({
			title : 'Credits',
			href : 'http://www.webmd.com/pain-management/knee-pain/tc/patellar-tracking-disorder-credits'
			});
            	
	
	/* set Current topic */	
	jQuery.each(topics, function(i, val) {
    if (window.location.href.indexOf(topics[i].href) > -1){
	   currentTopic = i;
	   }
    });
	

	/* if on the first article in a set, remove the previous navigation */
		if ( currentTopic == 0 ) {
			$(".pagination_fmt .left_fmt .outline_fmt").hide();
		}
		if ( window.location.href.indexOf("page=") > 1 ) {
			$(".pagination_fmt .left_fmt .outline_fmt").show();
		}

	
	/* if on the last article in a set, remove the next navigation */
	if ( (currentTopic + 1) == topics.length) {
			$(".pagination_fmt .right_fmt .outline_fmt").hide();
		
		}

/* Inserting Previous */
   if (currentTopic != 0){
   var a_hrefl = $('.pagination_fmt .left_fmt .outline_fmt a').attr('href');
   if (typeof(a_hrefl) === "undefined"){
   	$(".pagination_fmt .left_fmt .outline_fmt a").attr("href" , a_hrefl);
   } else {
    $(".pagination_fmt .left_fmt .outline_fmt a").attr("href" , topics[currentTopic - 1].href);
    }
    }


		
	/* Inserting Next */
   if ( (currentTopic + 1) != topics.length ) {
   var a_hrefr = $('.pagination_fmt .right_fmt .outline_fmt a').attr('href');
   if (typeof(a_hrefr) === "undefined"){
   $(".pagination_fmt .right_fmt .outline_fmt a").attr("href" , a_hrefr);
   }
   else if ( window.location.href.indexOf("check-your-symptoms") > -1 ) {
			$(".pagination_fmt .right_fmt .outline_fmt a").attr("href" , topics[currentTopic + 1].href);
		}
   else if (a_hrefr.indexOf('page') != -1 ) {}
   else {$(".pagination_fmt .right_fmt .outline_fmt a").attr("href" , topics[currentTopic + 1].href);}
	}
   
			
	/* Secondary pages with no data move to the next page.  */
			
	/* adding metrics to nav */
	$(".right_fmt a").on("click", function() {
		return sl(this,'','pagenum-art_next');
	});
	$(".left_fmt a").on("click", function() {
		return sl(this,'','pagenum-art_prev');
	});
	
});</script>
